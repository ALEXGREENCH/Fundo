<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/sportsHistory_mian_bg"
    android:orientation="vertical">

    <RelativeLayout
        android:background="?attr/global_background"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title"
        android:layout_gravity="center"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <TextView
            android:id="@+id/tv_sporthistory_title"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="游泳"
            android:textColor="?attr/global_text_color"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/back"
            android:layout_width="45dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:scaleType="centerInside"
            android:src="?attr/global_title_return_back" />
        <ImageButton
            android:id="@+id/ib_sporthistory_share"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="15dp"
            android:scaleType="centerInside"
            android:layout_centerVertical="true"
            android:background="@null"
            android:layout_alignParentEnd="true"
            android:src="?attr/sport_share_background" />

        <ImageButton
            android:id="@+id/ib_sporthistory_photo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:scaleType="centerInside"
            android:layout_centerVertical="true"
            android:background="@null"
            android:layout_toStartOf="@id/ib_sporthistory_share"
            android:src="?attr/sport_photo_background" />
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line"
            android:layout_alignParentBottom="true"
            android:background="?attr/health_reports_head_line_color" />
</RelativeLayout>

    <ScrollView
        android:id="@+id/detailfragment_sc"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:id="@+id/rl_spped_head"
                android:layout_width="match_parent"
                android:layout_height="220dp"
                android:layout_marginBottom="6dp"
                android:layout_marginLeft="6dp"
                android:layout_marginRight="6dp"
                android:layout_marginTop="50dp"
                android:background="?attr/speedhead_bg">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:layout_alignParentBottom="true"
                    android:orientation="horizontal">
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <com.szkct.weloopbtsmartdevice.view.DIN1451EF_EngNeuTypefaceTextView
                            android:id="@+id/tv_showdic"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:paddingTop="40dp"
                            android:text="222"
                            android:textColor="?attr/runtext_color"
                            android:textSize="40sp" />
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/tv_showdic"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/detailed_mileage_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:paddingEnd="10dp"
                                android:text="@string/detailed_mileage"
                                android:textColor="?attr/runtext_color"
                                android:textSize="20sp" />
                            <TextView
                                android:id="@+id/tv_showdic_up"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/kilometer"
                                android:textColor="?attr/runtext_color"
                                android:textSize="15sp" />
                        </LinearLayout>
                    </RelativeLayout>

                    <View
                        android:layout_width="2px"
                        android:layout_height="match_parent"
                        android:background="?attr/sportsHistory_mian_bg" />

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/tv_showtime"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/total_length_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:paddingEnd="10dp"
                                android:text="@string/total_length"
                                android:textColor="?attr/runtext_color"
                                android:textSize="20sp" />

                            <TextView
                                android:id="@+id/realTime_hms_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/realtime_hms"
                                android:textColor="?attr/runtext_color"
                                android:textSize="15sp" />
                        </LinearLayout>

                        <com.szkct.weloopbtsmartdevice.view.DIN1451EF_EngNeuTypefaceTextView
                            android:id="@+id/tv_showtime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:paddingTop="40dp"
                            android:text="222"
                            android:textColor="?attr/runtext_color"
                            android:textSize="40sp" />
                    </RelativeLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2px"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="150dp"
                    android:background="?attr/sportsHistory_mian_bg" />

                <LinearLayout
                    android:id="@+id/ll_detailedtop"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="140dp"
                    android:background="?attr/speedhead_logo_bg"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/sportmode_twopage_logo_iv"
                        android:layout_width="26dp"
                        android:layout_height="26dp"
                        android:paddingEnd="5dp"
                        android:src="@drawable/sportmode_logo_b" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="@string/funfit_sprots"
                        android:textColor="?attr/runtext_color" />

                    <TextView
                        android:id="@+id/detail_date"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:paddingStart="5dp"
                        android:paddingEnd="5dp"
                        android:textColor="?attr/runtext_color"
                        android:textSize="12sp" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_detailed_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/ll_detailedtop"
                    android:layout_centerHorizontal="true"
                    android:paddingBottom="10dp"

                    android:textColor="?attr/runtext_gray_color"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/detail_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/ll_detailedtop"
                    android:layout_marginTop="8dp"
                    android:gravity="center"
                    android:textColor="?attr/runtext_color" />
            </RelativeLayout>

            <com.szkct.map.utils.CircleImageView
                android:id="@+id/detail_icon"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="28dp"
                android:src="@drawable/head_men"
                app:border_color="@color/bule_bg"
                app:border_width="1dp" />

            <LinearLayout
