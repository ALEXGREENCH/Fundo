                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="@dimen/detailed_bottompadtop"
                            android:text="@string/youyong_ci"
                            android:textColor="?attr/runtext_gray_color"
                            android:textSize="@dimen/detailed_bottomtvsize" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="@dimen/detailed_itemheight"
                        android:layout_weight="1"
                        android:gravity="center_vertical|left"
                        android:orientation="vertical"
                        android:paddingStart="@dimen/detailed_paddingStart">
                        <com.szkct.weloopbtsmartdevice.view.AlwaysMarqueeTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="marquee"
                            android:marqueeRepeatLimit="marquee_forever"
                            android:paddingBottom="@dimen/detailed_toppadbottom"
                            android:singleLine="true"
                            android:text="@string/youyong_zyyhs"
                            android:textColor="?attr/runtext_gray_color"
                            android:textSize="@dimen/detailed_toptvsize"
                            android:focusableInTouchMode="true"
                            android:clickable="false"
                            />

                      <!--  android:text="@string/detailed_training_intensity"-->

                        <com.szkct.weloopbtsmartdevice.view.DIN1451EF_EngNeuTypefaceTextView
                            android:id="@+id/tv_zzyhscs"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="?attr/runtext_color"
                            android:textSize="@dimen/detailed_centvsize" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="@dimen/detailed_bottompadtop"
                            android:text="@string/youyong_ci"
                            android:textColor="?attr/runtext_gray_color"
                            android:textSize="@dimen/detailed_bottomtvsize" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line"
                    android:layout_marginLeft="@dimen/detailed_line_paddingStart"
                    android:layout_marginRight="@dimen/detailed_line_paddingStart"
                    android:background="@color/sports_line_color" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="@dimen/detailed_itemheight"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:paddingStart="@dimen/detailed_paddingStart">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="@dimen/detailed_toppadbottom"
                            android:text="@string/youyong_qtyzhs"
                            android:textColor="?attr/runtext_gray_color"
                            android:textSize="@dimen/detailed_toptvsize" />

                        <!--android:text="@string/detailed_max_pace"-->

                        <com.szkct.weloopbtsmartdevice.view.DIN1451EF_EngNeuTypefaceTextView
                            android:id="@+id/tv_qtyzhscs"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="?attr/runtext_color"
                            android:textSize="@dimen/detailed_centvsize" />

                        <com.szkct.weloopbtsmartdevice.view.AlwaysMarqueeTextView
                            android:id="@+id/detail_zgps_up"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ellipsize="marquee"
                            android:marqueeRepeatLimit="marquee_forever"
                            android:paddingBottom="@dimen/detailed_bottompadtop"
                            android:singleLine="true"
                            android:text="@string/youyong_ci"
                            android:textColor="?attr/runtext_gray_color"
                            android:textSize="@dimen/detailed_bottomtvsize" />

                    </LinearLayout>
